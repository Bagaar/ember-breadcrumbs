{
  "name": "@bagaar/ember-breadcrumbs",
  "version": "1.0.0-beta.0",
  "description": "Template based breadcrumb management for Ember applications.",
  "keywords": [
    "bagaar",
    "breadcrumbs",
    "ember",
    "ember-addon"
  ],
  "repository": "https://github.com/Bagaar/ember-breadcrumbs",
  "license": "MIT",
  "author": "Bagaar",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "format:js": "prettier-standard './**/*.js' && eslint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "ember-cli-babel": "^7.13.0",
    "ember-cli-htmlbars": "^4.2.0",
    "ember-in-element-polyfill": "^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@ember/optional-features": "^1.1.0",
    "@glimmer/component": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.15.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.15.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-ember": "^7.7.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "loader.js": "^4.7.0",
    "prettier-standard": "^15.0.1",
    "qunit-dom": "^0.9.2",
    "standard-version": "^7.0.1"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
