{
  "private": true,
  "repository": "https://github.com/bagaar/ember-breadcrumbs",
  "license": "MIT",
  "author": "Bagaar",
  "scripts": {
    "build": "pnpm --filter ember-breadcrumbs build",
    "format": "pnpm --filter '*' format",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefixColors auto",
    "start:addon": "pnpm --filter ember-breadcrumbs start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember"
  },
  "devDependencies": {
    "@glint/core": "^1.2.1",
    "concurrently": "^9.1.2",
    "prettier": "^3.0.3",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "release-plan": "^0.16.0"
  },
  "packageManager": "pnpm@10.8.1",
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.8.1"
  }
}
