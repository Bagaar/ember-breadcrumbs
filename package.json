{
  "name": "workspace-root",
  "version": "4.1.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "workspaces": [
    "ember-breadcrumbs",
    "test-app"
  ],
  "scripts": {
    "build": "yarn workspace @bagaar/ember-breadcrumbs run build",
    "build:js": "yarn workspace @bagaar/ember-breadcrumbs run build:js",
    "build:types": "yarn workspace @bagaar/ember-breadcrumbs run build:types",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "prepare": "husky install",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "yarn workspace @bagaar/ember-breadcrumbs run start",
    "start:test-app": "yarn workspace test-app run start",
    "test": "yarn workspace test-app run test"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "concurrently": "^8.2.1",
    "husky": "^8.0.3",
    "standard-version": "^9.5.0"
  }
}